;AAAjB,SAFI;AAPJ;AARD,KAAP;AAqBD,GA9BY;AAgCf;AACE,EAAA,KAAK,EAAC;AACJ,IAAA,SADI,qBACM,cADN,EACsB,cADtB,EACqC;AACvC,UAAG,cAAa,GAAI,cAApB,EAAmC;AACjC,QAAA,OAAO,CAAC,GAAR,WAAe,KAAK,IAAL,CAAU,QAAzB;AACF;AACF;AALI,GAjCO;AAyCf;AACE,EAAA,QAAQ,EAAE;AACR,IAAA,sBADQ,oCACgB;AACpB,aAAO,KAAK,eAAL,CAAqB,MAA5B;AACJ;AAHQ,GA1CG;AAgDf;AACE,EAAA,OAAO,EAAE;AACP,IAAA,UADO,wBACK;AACV,WAAK,SAAL;AACD,KAHM;AAIP,IAAA,cAJO,0BAIQ,EAJR,EAIW;AACd,MAAA,OAAO,CAAC,GAAR,4BAAgC,EAAhC;AACH,KANM;AAOP,IAAA,cAPO,4BAOS;AACZ,UAAG,KAAK,eAAL,IAAwB,KAAK,iBAAL,KAA2B,OAAtD,EAA8D;AAC1D,aAAK,IAAL,CAAU,MAAV,CAAiB,OAAjB,CAAyB;AACrB,UAAA,EAAE,EAAE,KAAK,IAAL,CAAU,MAAV,CAAiB,MAAjB,GAA0B,CADT;AAErB,UAAA,OAAO,EAAE,KAAK;AAFO,SAAzB,EAD0D,CAK1D;;AACA,aAAK,eAAL,GAAuB,EAAvB;AACJ;AACJ;AAhBO,GAjDI;AAoEf;AACE,EAAA,OArEa,qBAqEJ;AACP,SAAK,UAAL;AACF;AAvEa,CAAf","sourcesContent":["<template>\n    <div class='user-profile'>\n        <div class='user-panel'>\n            <h1 class='username'>@{{user.username}}</h1>\n            <div class='admin-badge' v-if=\"user.isAdmin\">\n                Admin\n            </div>\n            <div class='follow-count'>\n                <strong>Followers: </strong> {{followers}}\n            </div> \n            <form class='create-twoot' @submit.prevent=\"createNewTwoot\">\n                <label for=\"newTwoot\"><strong>New Twoot</strong> ({{newTwootCharacterCount}}/180)</label>\n                <textarea id=\"newTwoot\" row=\"4\" v-model=\"newTwootContent\"/>\n                <div class='create-twoot-type'>\n                    <label for=\"newTwootType\"><strong>Type: </strong></label>\n                    <select id=\"newTwootType\" v-model=\"selectedTwootType\">\n                        <option \n                            v-for=\"(option, index) in twootTypes\"\n                            :key=\"index\"\n                            :value=\"option.value\"\n                        >\n                            {{option.name}}\n                        </option>\n\n                    </select>\n                </div>\n                <button>\n                    Twoot!\n                </button>\n            </form>\n        </div>\n        <div class='twoots-wrapper'>\n            <TwootItem \n                v-for=\"twoot in user.twoots\" \n                :key=\"twoot.id\" \n                :username=\"user.username\" \n                :twoot=\"twoot\"\n                @favorite=\"toggleFavorite\"\n            />\n        </div>\n    </div>\n\n</template>\n\n<script>\nimport TwootItem from './TwootItem';\n\nexport default {\n  name: 'UserProfile',\n\n  components: {\n      TwootItem\n  },\n\n// This is the state ... kinda\n  data() {\n    return {\n      newTwootContent: '',\n      selectedTwootType: 'instant',\n      twootTypes:[\n          {value: 'draft', name: 'Draft'},\n          {value: 'instant', name: 'Instant Twoot'}\n      ],\n      followers: 0,\n      user: {\n        id: 1,\n        username: 'BennettBierman',\n        firstName: 'Bennett',\n        lastName: 'Bierman',\n        email: 'Bbierman07@gmail.com',\n        isAdmin: true ,\n        twoots: [\n            {id: 1, content: 'Twotter is Amazing!'},\n            {id: 2, content: 'Lets go Giants'}\n        ]\n      }\n    }\n  },\n\n//These methods watch for events\n  watch:{\n    followers(newFollowCount, oldFollowCount){\n      if(oldFollowCount < newFollowCount){\n        console.log(`${this.user.username} has gained a follower `)\n      }\n    }\n  },\n\n//These methods compute things like FullName or NumberOfCharacters\n  computed: {\n    newTwootCharacterCount(){\n        return this.newTwootContent.length;\n    }\n  },\n\n//Other general methods\n  methods: {\n    followUser(){\n      this.followers++;\n    },\n    toggleFavorite(id){\n        console.log(`Favorited Twoot #${id}`)\n    },\n    createNewTwoot(){\n        if(this.newTwootContent && this.selectedTwootType !== 'draft'){\n            this.user.twoots.unshift({\n                id: this.user.twoots.length + 1,\n                content: this.newTwootContent\n            });\n            // this.user.twoots.push({...}) adds twoot to end of the list rather than the front\n            this.newTwootContent = '';\n        }\n    }\n  },\n\n//Method that runs when the component is mounted\n  mounted(){\n    this.followUser()\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.user-profile {\n    display: grid;\n    grid-template-columns: 1fr 3fr;\n    width: 100%;\n    padding: 50px 5%;\n    .user-panel{\n        display: flex;\n        flex-direction: column;\n        margin-right: 50px;\n        padding: 20px;\n        background-color: white;\n        border-radius: 5px;\n        border: 1px solid #DFE3E8;\n        align-items: flex-start;\n\n        h1 {\n            margin: 0;\n        }\n        \n        .admin-badge{\n            background-color: rebeccapurple;\n            color: white;\n            border-radius: 5px;\n            margin-right: auto;\n            padding: 0 10px;\n            font-weight: bold;\n\n        }\n        .create-twoot{\n            display: flex;\n            flex-direction: column;\n            padding-top: 20px;\n        }\n    }\n    .twoots-wrapper{\n        display: grid;\n        grid-gap:10px;\n    }\n}\n</style>"],"sourceRoot":""}]}